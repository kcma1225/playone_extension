{
  "manifest_version": 3,
  "name": "Playone_plugins",
  "description": "mulit plugins for PLAYONE",
  "version": "1.0.0",
  "permissions": [ "cookies", "scripting", "activeTab", "storage"],
  "host_permissions": [ "<all_urls>"],

  "action": {
    "default_popup": "popup/popup.html",
    "default_icon" : "assets/icon/icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module" 
  }
}
